@article{Hastie-Tibshirani-1993,
	Abstract = {We explore a class of regression and generalized regression models
	in which the coefficients are allowed to vary as smooth functions
	of other variables. General algorithms are presented for estimating
	the models flexibly and some examples are given. This class of models
	ties together generalized additive models and dynamic generalized
	linear models into one common framework. When applied to the proportional
	hazards model for survival data, this approach provides a new way
	of modelling departures from the proportional hazards assumption.},
	Author = {Hastie, Trevor and Tibshirani, Robert},
